    <!-- ヘッダー終了 -->

    <main class = "container">
      <div id ="main_content">
        <section id = "campaign">
          <h1>コンタクト</h1>
          <p><%= message1 %></p>
          <p class = "message_teller"><%= message2 %></p>
        </section>

        <div id ="form_content">
          <form action="/contact" method="post" id = "form_detail">
            <div class="form-group">
              <label for="exampleInputname">氏名</label>
              <input type = "name" name="name" class="form-control" id="exampleInputname" aria-dscribedby="nameHelp" placeholder="山田太郎" required>
              <p id="error-name" class="error-message"></p>
            </div>
            <div class="form-group">
              <label for="exampleInputEmail">メールアドレス</label>
              <input type = "email" name="email" class="form-control" id="exampleInputEmail" aria-describedby="emailHelp" placeholder="taro.yamada@gmail.com" required>
              <p id="error-email" class="error-message"></p>
            </div>
            <div class="form-group">
              <label for="exampleInputSubject">件名</label>
              <input type = "subject" name="subject" class="form-control" id="exampleInputSubject" aria-describedby="subjectHelp" placeholder="ここには自由にコメントを書いてください。" required>
              <p id="error-subject" class="error-message"></p>
            </div>
            <div class="form-group">
              <label for="exampleInputMessage">お問い合わせ内容</label>
              <textarea type = "message" name ="message" class="form-control" id="exampleInputMessage" rows="15" placeholder="ここには自由にコメントを書いてください。" required></textarea>
              <p id="error-message" class="error-message"></p>
            </div>
            <div class="form-group">
              <button type="submit" class="btn btn-primary">送信</button>
            </div>
          </form>
        </div>
      </div>
    </main>


    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
